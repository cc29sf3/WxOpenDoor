<!--pages/admin/admin.wxml-->
<i-button class="bottom-btn" bind:click="addSecurity" inline>添加</i-button>
<button bindtap="init" class="bottom-btn">init</button>
<iview wx:if="{{securities}}">
暂无人员
</iview>
<i-swipeout class="i-swipeout-demo-item" wx:for="{{securities}}" wx:key="id" data-user="{{item}}" actions="{{actions}}" bind:change="swipeoutClick">
    <view slot="content">
        <i-cell 
            i-class="i-cell-padding" 
            title="{{item.name}}" 
            label="手机号码:{{item.phone}}">
        </i-cell>
    </view>
</i-swipeout>
<i-toast id="toast" />


